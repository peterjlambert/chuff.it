---
layout: layouts/main
---

<main role="main">
  <section class="mainContent">
    <header role="banner" class="mainHeader">
      <h1 class="mainHeader__headline">
        {{ headline }}
      </h1>
      <p class="mainHeader__description">
        {{ meta.description }}
      </p>
    </header>
    <article class="page__text">
      {{ content | safe }}
    </article>
  </section>
</main>

